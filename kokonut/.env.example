# Environment Variables Example

# Application settings
APP_NAME=Kokonut
APP_ENV=development
APP_DEBUG=true
APP_PORT=8080

# Database settings
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=kokonut
DB_USERNAME=root
DB_PASSWORD=secret

# Cache settings
CACHE_DRIVER=file
CACHE_PATH=/path/to/cache

# Mail settings
MAIL_DRIVER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
